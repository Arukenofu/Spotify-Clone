{
  "name": "spotify-clone",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:client": "vite --host",
    "dev:server": "tsx --watch server/index.ts",
    "dev:all": "run-p dev:client dev:server",
    "build:client": "vite build",
    "build:client:check": "run-p type-check \"build:client {@}\" --",
    "prod": "cross-env NODE_ENV=production vite-node ./server/index.ts",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.16.12",
    "@fastify/cors": "^11.0.1",
    "@fastify/formbody": "^8.0.2",
    "@fastify/send": "^4.1.0",
    "@fastify/static": "^8.2.0",
    "@nuclearplayer/ytdl-core": "^4.16.13",
    "@spotify/web-api-ts-sdk": "^1.2.0",
    "@tanstack/vue-query": "^5.66.0",
    "@types/cors": "^2.8.19",
    "cors": "^2.8.5",
    "fast-average-color": "^9.5.0",
    "fastify": "^5.3.3",
    "musicxmatch-api": "^1.0.1",
    "node-vibrant": "^4.0.0",
    "ofetch": "^1.4.1",
    "pinia": "^2.3.1",
    "spottydl": "^0.2.7",
    "tsx": "^4.20.3",
    "vue": "^3.5.13",
    "vue-i18n": "11.1.2",
    "vue-router": "^4.5.0",
    "vue-tippy": "^6.5.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.28.0",
    "nodemon": "^3.1.10",
    "npm-run-all2": "^6.2.2",
    "sass": "^1.79.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "unplugin-vue-router": "^0.11.2",
    "vite": "^5.4.8",
    "vite-node": "^3.2.2",
    "vite-plugin-compression2": "^1.3.0",
    "vite-plugin-sass-glob-import": "^3.0.2",
    "vite-plugin-vue-devtools": "^7.5.6",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^2.1.6"
  }
}
