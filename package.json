{
  "name": "spotify-clone",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:client": "vite --host",
    "dev:server": "tsx --watch server/index.ts",
    "dev:all": "run-p dev:client dev:server",
    "build:client": "vite build",
    "build:client:check": "run-p type-check \"build:client {@}\" --",
    "prod": "cross-env NODE_ENV=production tsx ./server/index.ts",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.16.12",
    "@fastify/cors": "^11.0.1",
    "@fastify/formbody": "^8.0.2",
    "@fastify/send": "^4.1.0",
    "@fastify/static": "^8.2.0",
    "@jayimbee/palette": "^1.0.7",
    "@nuclearplayer/ytdl-core": "^4.16.13",
    "@spotify/web-api-ts-sdk": "^1.2.0",
    "@tanstack/vue-query": "^5.80.7",
    "@types/yt-search": "^2.10.3",
    "cors": "^2.8.5",
    "fastify": "^5.3.3",
    "musicxmatch-api": "^1.0.1",
    "ofetch": "^1.4.1",
    "pinia": "^2.3.1",
    "vue": "^3.5.16",
    "vue-i18n": "11.1.2",
    "vue-router": "^4.5.0",
    "vue-tippy": "^6.5.0",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "yt-search": "^2.13.1",
    "ytmusic-api": "^5.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@tsconfig/node20": "^20.1.6",
    "@types/cors": "^2.8.19",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "nodemon": "^3.1.10",
    "npm-run-all2": "^6.2.2",
    "sass": "^1.79.3",
    "tsx": "^4.20.3",
    "typescript": "~5.6.2",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.3.5",
    "vite-plugin-compression2": "^2.0.1",
    "vite-plugin-sass-glob-import": "^5.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vite-plugin-vue-layouts-next": "^0.1.5",
    "vue-tsc": "^2.1.6"
  }
}
